  "scripts": {
    "build": "gradle build -x test",
    "build:static": "mkdir -p build/static && cp -r static-site/* build/static/",
    "build:webpack": "webpack --mode production",
    "build:tailwind": "tailwindcss -i ./src/main/resources/static/css/input.css -o ./src/main/resources/static/css/output.css --watch",
    "dev:webpack": "webpack serve --mode development",
    "dev:ai": "cd ai-service && python app.py",
    "start:ai": "cd ai-service && pip install -r requirements.txt && python app.py",
    "deploy": "surge build/static/ --domain adhyayan-sangha.surge.sh",
    "deploy:new": "npm run build:static && surge build/static/",
    "deploy:auto": "./deploy-surge.sh",
    "test": "echo \"No tests specified\" && exit 0"
  },
